{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "outDir": "dist",
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "node",
      "react",
      "styles-components",
      "vite/client",
      "vite-plugin-svgr/client"
    ]
  },
  "include": [
    "src"
  ],
  "files": [
    "vite.config.ts"
  ]
}
